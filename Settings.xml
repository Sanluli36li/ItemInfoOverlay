<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <Script file="Settings.lua"/>
    <Frame name="SanluliItemInfoOverlaySettingPriviewTemplate" mixin="SanluliItemInfoOverlaySettingPriviewMixin" virtual="true">
		<Size y="60"/>
		<Frames>
			<ItemButton parentKey="itemButton1">
				<Anchors>
					<Anchor point="TOPLEFT" x="40" y="-20"/>
				</Anchors>
			</ItemButton>
			<ItemButton parentKey="itemButton2">
				<Anchors>
					<Anchor point="LEFT" relativeKey="$parent.itemButton1" relativePoint="RIGHT" x="5" y="0"/>
				</Anchors>
			</ItemButton>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>

    <Frame name="SanluliCharacterFrameItemInfoOverlaySettingPriviewTemplate" mixin="SanluliCharacterFrameItemInfoOverlaySettingPriviewMixin" virtual="true">
		<Size y="60"/>
		<Frames>
			<ItemButton parentKey="itemButton">
				<Anchors>
					<Anchor point="TOPLEFT" x="40" y="-20"/>
				</Anchors>
			</ItemButton>
		</Frames>
		<Scripts>
			<OnLoad method="OnLoad"/>
		</Scripts>
	</Frame>
</Ui>